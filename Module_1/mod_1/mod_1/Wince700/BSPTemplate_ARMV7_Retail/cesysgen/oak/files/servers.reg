; @CESYSGEN IF SERVERS_MODULES_DSTSVC
; Have DST changes automatically enabled
[HKEY_LOCAL_MACHINE\Services\DSTSVC]
   "Dll"="dstsvc.dll"
   "Order"=dword:99
   "Keep"=dword:1
   "Prefix"="DST"
   "Index"=dword:0
   "UserProcGroup"=dword:$(PROCGROUP_SERVICE_MSFT_DEFAULT) ; run in microsoft group instead of the default group


[HKEY_LOCAL_MACHINE\SOFTWARE\Microsoft\Clock]
"AutoDST"=dword:1
